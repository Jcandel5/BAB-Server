<% include ../partials/header %>

<div class="container">
  <header class="jumbotron">
    <div class="container">
      <h1>Coupons!</h1>
      <p>
        <a class="btn btn-primary btn-lg" href="/admin/coupons/add">New Coupons</a>
      </p>
    </div>

  </header>
 <div class="row text-center"  style="display:flex; flex-wrap:wrap">


   <% coupons.forEach(coupon => { %>
     <div class="col-md-3 col-sm-6">
       <div class="thumbnail">
         <img src="<%= coupon.image %>" alt="data:image/png;base64," width="200px">
         <br>
         <form class="delete-form" action="/admin/coupons/delete/<%= coupon._id %>?_method=DELETE" method="POST">
             <input class="btn btn-xs btn-danger" type="submit" name="delete" value='Delete' onClick="return confirm('Are you sure you want to delete?')" />
         </form>
       </div>
     </div>

   <%})%>
 </div>

</div>
